{
  "name": "extrachill-community",
  "version": "1.1.2",
  "description": "Extra Chill community platform - WordPress theme for forums and community features",
  "main": "style.css",
  "scripts": {
    "build": "wp-scripts build --webpack-copy-php",
    "start": "wp-scripts start",
    "lint:js": "wp-scripts lint-js",
    "lint:css": "wp-scripts lint-style",
    "format": "wp-scripts format",
    "zip": "./build.sh",
    "version": "grep -i 'Version:' style.css | head -1 | sed 's/.*Version:\\s*\\([0-9\\.]*\\).*/\\1/'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chubes4/extrachill-community.git"
  },
  "keywords": [
    "wordpress",
    "theme",
    "community",
    "forum",
    "bbpress",
    "music",
    "extrachill"
  ],
  "author": "Chris Huber <hello@chubes.net> (https://chubes.net)",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/chubes4/extrachill-community/issues"
  },
  "homepage": "https://community.extrachill.com",
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@wordpress/scripts": "^30.15.0"
  },
  "dependencies": {
    "@wordpress/api-fetch": "^7.0.0",
    "@wordpress/block-editor": "^14.0.0",
    "@wordpress/blocks": "^15.9.0",
    "@wordpress/components": "^28.0.0",
    "@wordpress/data": "^10.36.0",
    "@wordpress/element": "^6.0.0",
    "@wordpress/i18n": "^5.0.0",
    "@wordpress/icons": "^10.0.0"
  },
  "private": true
}
